{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "typeRoots": ["./src/global.d.ts", "./node_modules/@types"],
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir":"./dist",
    "strict": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
